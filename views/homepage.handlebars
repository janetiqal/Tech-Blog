
{{#each posts as |post|}}
   <div class="card card-post">
        <h2 class="card-header">Title: {{title}}</h2>
           
            <p>{{post_body}}</p>

            <p>Written by: {{user.username}} {{format_date created_at}}</p>
            <p>{{post comments}}</p>
            {{!-- <p>{{comment.body}}</p> --}}
    </div>
{{/each}}
